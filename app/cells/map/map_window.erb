<div id="osx-modal-content" style="display:none"> 
  <div id="osx-modal-title">添加我的地点</div> 
  <div class="close"><a href="#" class="simplemodal-close">x</a></div> 
  <div id="osx-modal-data"> 
    <div id = "place_tag" style="padding-bottom: 5px;padding-top: 10px;">
      <div>
        <%= javascript_include_tag "extend/map" %>
        <%= javascript_include_tag "map/markercluster" %>
        <%= javascript_include_tag "extend/jquery.form" %>
        <%= javascript_include_tag  'http://maps.google.com/maps/api/js?sensor=false' %>
        <%= javascript_include_tag  'map/map_controll' %>
        <script src="http://www.google.com/uds/api?file=uds.js&amp;v=1.0&amp;key=ABQIAAAAjU0EJWnWPMv7oQ-jjS7dYxQ82LsCgTSsdpNEnBsExtoeJv4cdBSUkiLH6ntmAr_5O4EfjDwOa0oZBQ" type="text/javascript"></script> 
        <%= javascript_include_tag 'map/map_search' %>
        <script type="text/javascript">
          $(document).ready(function() {
            MapObject.map_share_html = $("#map_infowindow").html()
            alert(MapObject.map_share_html)
            <%#*$('#map_infowindow').remove()%>
<% if @map.center_latitude %> MapObject.map_options.center_latitude = <%= @map.center_latitude%>;<%end%>
<% if  @map.center_longitude %> MapObject.map_options.center_longitude = <%= @map.center_longitude%>;<%end%>
<% if @map.zoom %>MapObject.map_options.zoom = <%= @map.zoom%>;<%end%>
    
    MapObject.initialize(null) ;
  });
        </script>
        <div id ="map_infowindow" style="display: none" >
          <%= render_cell :common, :new_place %>
        </div>
        <div id="map_controll">
          <ul class="buttons">
            <li><span class="button" id="new-marker-tab" onclick="MapObject.add_marker_listen();">发布地点</span></li>
            <li><span class="button" onclick="MapObject.init_marker_from_data('firend_postition');">查看好友位置</span></li>
          </ul>
        </div>
        <div id="mapCanvas" style="width: 100%; height: 385px;">map div</div> 
      </div>
    </div> 
  </div> 
</div>
<!-- modal content --> 


