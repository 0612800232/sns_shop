//alert('<%=@marker.id%>')


if(MapObject.markers.get(<%=@marker.id%>)){
MapObject.add_info_to_marker_from_data(<%=@marker.id%>,MapObject.markers.get(<%=@marker.id%>),'<%= raw mark_html(@marker) %>',true);	
}else{
MapObject.add_info_to_marker_from_data(<%=@marker.id%>,MapObject.temp_marker,'<%= raw mark_html(@marker) %>',true);
MapObject.markers.put(<%=@marker.id%>,MapObject.temp_marker);
}


