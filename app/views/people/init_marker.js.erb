<%if @markers.each do|marker|%>


marker = new google.maps.Marker({
				    map: MapObject.map,
				    draggable: false,
				    animation: google.maps.Animation.DROP,
				    position: new google.maps.LatLng(<%=marker.marker_latitude%>, <%= marker.marker_longitude %>)
						});
MapObject.add_info_to_marker_from_data(<%=marker.id%>,marker,'<%= raw mark_html(marker) %>',false);
MapObject.markers.put(<%=marker.id%>,marker);

<%end%>
<%end%>


