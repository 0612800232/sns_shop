<%= content_for  :head do %>
  <%= stylesheet_link_tag 'global-0.52' %>
  <%= stylesheet_link_tag  "osx" %>
  <%= stylesheet_link_tag  "miaov_style" %>
  <%= javascript_include_tag  "tag_cloude/miaov" %>
<% end %>


<div class="first_div" style="height: 381px;">
  <div class="left" style="float: left;width: 285px;margin: 0 40px 00px 0;padding: 0 25px 25px 0;background-color: white;">
    <%= render_cell :place,:search %>
    <%= render_cell :place,:search_by_tag %>
  </div>
  <div class="right" style="margin-left: 310px;width: 650px;">
    <%= render_cell :place,:hot%>
  </div>
</div>

<div class="split"></div>

<div class="second_div">
  <div id="link_tag" style="margin-bottom:10px;">
    <%= link_to "视界","#" %>
    <span style="float: right;margin-right: 60px;">
      <%= link_to "情侣","#" %>
      <%= link_to "风景","#" %>
      <%= link_to "美女","#" %>
      <%= link_to "童真","#" %>
      <%= link_to "家庭","#" %>
    </span>
  </div>
  <div id="window_content">
    <div style="" class="qiji"> 
      <%= link_to  image_tag('missing_small.png',:class=>'pic-show',:alt =>"奇妙之旅"), "#",:target=>"_blank" %>
      <%for i in 0..5 %>
        <%= link_to  image_tag('clean/manoutline.png',:class=>'pic-show',:alt =>"图片",:height=>"75px"), "#",:target=>"_blank" %>
      <%end%>
    </div>
  </div>
  <div id="window_content">
    <div style="" class="fengguan"> 
      <%= link_to  image_tag('missing_small.png',:class=>'pic-show',:alt =>"风光无限"), "#",:target=>"_blank" %>
      <%for i in 0..5 %>
        <%= link_to  image_tag('clean/manoutline.png',:class=>'pic-show',:alt =>"图片",:height=>"75px"), "#",:target=>"_blank" %>
      <%end%>
    </div>
  </div>
  <div id="window_content">
    <div style="" class="wanjia"> 
      <%= link_to  image_tag('missing_small.png',:class=>'pic-show',:alt =>"玩家上传"), "#",:target=>"_blank" %>
      <%for i in 0..5 %>
        <%= link_to  image_tag('clean/manoutline.png',:class=>'pic-show',:alt =>"玩家上传",:height=>"75px"), "#",:target=>"_blank" %>
      <%end%>
    </div>
  </div>
</div>



<div class="third_div" style="height:800px;">
  <div class="left" style="float: left;width: 550px;margin: 0 40px 00px 0;padding: 0 25px 25px 0;background-color: white;">
    <div id="meitu" style="height:250px;">
      <div style="float:left">
        <%= image_tag('missing_original.png',:alt =>"人物出行美图",:width=>"250px")%>
      </div>
      <div style="float:left">
        更多
      </div>
    </div>
    <br>
    <div id="shiping" style="height:300px;">
      <div style="float:left">
        <%= image_tag('missing_original.png',:alt =>"人物出行美图",:width=>"250px")%>
      </div>
      <div style="float:left">
        <span><p>视频标题</p></span>
        <span><p>视频标题</p></span>
        <span><p>视频标题</p></span>
