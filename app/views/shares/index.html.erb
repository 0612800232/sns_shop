        <span><p>视频标题</p></span>
        <span><p>视频标题</p></span>
      </div>
    </div>
    <div id="rizhi">
      <h1>精彩日志</h1>
      <div id="rizhi_ceng">
        <table>
          <%for i in 1..5%>
          <tr>
            <th style="width:225px">日志标题<%=i%></th>
            <th style="width:225px">日志标题<%=i+1%></th>
          <%end%>  
          </tr>
        </table>
      </div>
    </div>
  </div>
  <div class="right" style="margin-left: 600px;width: 650px;">
    <%render_cell,:share,:order %>
    <table style="height:250px;">
      <tr>
        <th style="font-size:20px" colspan=3>地方推荐排行</th>
      </tr>
      <%for i in 1..10%>
        <tr>
          <th><%=i%></th>
          <th>丽江</th>
          <th>推荐指数</th>
        </tr>
      <%end%>
    </table>
    <br>
    <table style="">
      <tr>
        <th style="font-size:20px" colspan=2>出行活动推荐</th>
      </tr>
      <%for i in 1..5%>
        <tr>
          <th><%= image_tag('missing_small.png',:alt =>"人物出行美图",:width=>"50px")%></th>
          <th>活动标题</th>
        </tr>
      <%end%>
    </table>
    <br>
    <div id="weibo" style="height:150px">微波</div>
    <div id="guanggao" style="height:150px;">广告</div>
  </div>
</div>

